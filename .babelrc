{
  "env": {
    "development": {
      "presets": [["es2015", { "modules": false }], "react"],
      "plugins": ["transform-class-properties", "transform-object-rest-spread"]
    },
    "production": {
      "presets": [["es2015", { "modules": false }], "react"],
      "plugins": ["transform-class-properties", "transform-object-rest-spread"]
    },
    "test": {
      "presets": ["es2015", "react"],
      "plugins": ["transform-class-properties", "transform-object-rest-spread"]
    }
  }
}
